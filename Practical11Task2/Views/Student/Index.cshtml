@model Practical11Task2.ViewModels.StudentViewModel
@{
    @if (Model.PageTitle == "Index")
    {
       await Html.RenderPartialAsync("~/views/partialview/_getallstudent.cshtml",Model.Students);
    }
    if (Model.PageTitle == "Create")
    {
       await Html.RenderPartialAsync("~/views/partialview/_Create.cshtml", Model.Student);
